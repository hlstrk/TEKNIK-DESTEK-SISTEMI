
@{
    ViewBag.Title = "Index";
}

<section class="content-header">
    <h1>

        <small>Mobil Uygulama Bildirimleri</small>
    </h1>
    <ol class="breadcrumb">
        <li><a href="#"><i class="fa fa-dashboard"></i> </a></li>

    </ol>
</section>
<br />




<div class="box box-default">


    <div class="box box-default">



        <div class="row" style="margin-right: 0px; margin-left:0px;"><div class="col-sm-12"></div><div class="col-sm-12"></div></div><div class="row" style="margin-right: 0px; margin-left:0px;">


            <div id="PanelMobilBildirimListesi" class="col-sm-12">

                @{
                    Html.RenderAction("BildirimListesi");


                }
            </div>
        </div>

    </div>

</div>





<div id="BildirimModal" class="modal">
    <div class="modal-dialog-scrollable" style="max-width: 520px; margin: 1.75rem auto;">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">Teknik/Arıza Kaydı Ekle Ve Düzenle</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" onclick="FormKapat('BildirimModal')">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>


            <div id="BildirimModalBody" class="modal-body">

            </div>

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>

<div id="BildirimSilModal" class="modal">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h4 class="modal-title">Destek Kaydını Sil</h4>
                <button type="button" class="close" data-dismiss="modal" aria-label="Kapat" onclick="FormKapat('BildirimSilModal')">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>


            <div id="BildirimSilModalBody" class="modal-body">

            </div>

        </div>
        <!-- /.modal-content -->
    </div>
    <!-- /.modal-dialog -->
</div>












